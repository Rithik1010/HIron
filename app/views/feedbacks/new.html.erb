<% provide(:title, 'Review') %>
<h1>Review</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@feedback, url: feed_path) do |f| %>
    
      <%= f.hidden_field :user_id, value: current_user.id %>
      <%= f.hidden_field :partner_id, value: params[:partner_id] %>
      <%= f.text_area :content, placeholder: "Write a review..." %>
      <%= f.submit "Submit Now", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>